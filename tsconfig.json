{
  "include": [
    "**/*",
    ".react-router/types/**/*"
  ],
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "rootDirs": [".", ".react-router/types"],
    "types": ["node", "vite/client"]
  }
}
